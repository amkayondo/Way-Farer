{
  "name": "way-farer",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking service.",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@hapi/joi": "^15.1.0",
    "@hapi/joi-date": "^1.3.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "core-js": "^3.1.4",
    "coverage": "^0.4.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "pg": "^7.12.1",
    "should": "^13.2.3",
    "swagger-ui-express": "^4.0.7"
  },
  "scripts": {
    "dev1": "cross-env NODE_ENV=development npm run dropdev && npm run devdb && npm run addadmin && nodemon --exec babel-node server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js",
    "dropdev": "cross-env NODE_ENV=development node --require @babel/register ./server/database/dropTables.js",
    "addadmin": "cross-env NODE_ENV=development node --require @babel/register ./server/database/createAdmin.js",
    "devdb": "cross-env NODE_ENV=development node --require @babel/register ./server/database/createTables.js",
    "test": "cross-env NODE_ENV=test npm run dropdb && npm run testdb && npm run testadmin && npm run runtest",
    "runtest": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --require @babel/register --timeout 100000 server/tests/main.test.js --exit",
    "dropdb": "cross-env NODE_ENV=test node --require @babel/register ./server/database/dropTables.js",
    "testdb": "cross-env NODE_ENV=test node --require @babel/register ./server/database/createTables.js",
    "testadmin": "cross-env NODE_ENV=test node --require @babel/register ./server/database/createAdmin.js",
    "prodadmin": "cross-env NODE_ENV=production node --require @babel/register ./server/database/createAdmin.js",
    "proddb": "cross-env NODE_ENV=production node --require @babel/register ./server/database/createTables.js",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "coverage": "nyc --reporter=lcov npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint",
    "build": "babel ./server/ --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amkayondo/Way-Farer.git"
  },
  "author": "kayondo edward",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amkayondo/Way-Farer/issues"
  },
  "homepage": "https://github.com/amkayondo/Way-Farer#readme"
}
